CARENCIA CONTRATO EMPRESTIMO

SELECT
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idclientesisbr AS "Código Cliente", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.BOLPESSOAFISICA AS "PF/PJ",
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.numcpfcnpj AS "CPF/CNPJ", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.DESCNOMECLIENTE AS "Nome", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idunidadeinst AS "Nº PA", REPCOOPCONS.CRE_CONTRATOMUTUO.NUMCONTRATOMUTUO AS "N°Contrato", 
REPCOOPCONS.CRE_CONTRATOMUTUO.QTDCARENCIA AS "Qtd Carência",
REPCOOPCONS.CRE_CONTRATOMUTUO.DATAMOVIMENTO AS "Dt Movimento"

FROM REPCOOPCONS.CLI_CLIENTEINSTUNIDADE, REPCOOPCONS.CRE_CONTRATOMUTUO

WHERE REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.IDCLIENTE =  REPCOOPCONS.CRE_CONTRATOMUTUO.IDCLIENTE AND
REPCOOPCONS.CRE_CONTRATOMUTUO.QTDCARENCIA > 150 AND REPCOOPCONS.CRE_CONTRATOMUTUO.DATAMOVIMENTO >'2020-01-01 00:00:00.0000000'

ORDER BY "Dt Movimento" DESC, "Qtd Carência" DESC


CARENCIA CONTRATO RURAL

SELECT
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idclientesisbr AS "Código Cliente", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.BOLPESSOAFISICA AS "PF/PJ",
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.numcpfcnpj AS "CPF/CNPJ", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.DESCNOMECLIENTE AS "Nome", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idunidadeinst AS "Nº PA", 
REPCOOPCONS.CRE_CONTRATORURAL.NUMCONTRATORURAL AS "N°Contrato", 
REPCOOPCONS.CRE_CONTRATORURAL.QTDCARENCIA AS "Qtd Carência",
REPCOOPCONS.CRE_CONTRATORURAL.DATAMOVIMENTO AS "Dt Movimento"

FROM REPCOOPCONS.CLI_CLIENTEINSTUNIDADE, REPCOOPCONS.CRE_CONTRATORURAL

WHERE REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.IDCLIENTE =  REPCOOPCONS.CRE_CONTRATORURAL.IDCLIENTE AND
REPCOOPCONS.CRE_CONTRATORURAL.QTDCARENCIA > 150 AND REPCOOPCONS.CRE_CONTRATORURAL.DATAMOVIMENTO >'2020-01-01 00:00:00.0000000'

ORDER BY "Dt Movimento" DESC, "Qtd Carência" DESC

CADASTRO SEM RENDA
SELECT
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idclientesisbr  AS "Código Cliente", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.BOLPESSOAFISICA  AS "PF/PJ", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.numcpfcnpj AS "CPF/CNPJ", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.DESCNOMECLIENTE AS "Nome", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idunidadeinst AS "Nº PA" FROM REPCOOPCONS.CLI_CLIENTEINSTUNIDADE
  

ORDER BY "Código Cliente"




SELECT
REPCOOPCONS.CLIENTEINSTUNIDADE.idclientesisbr AS "Código Cliente", REPCOOPCONS.clienteinstunidade.BOLPESSOAFISICA AS "PF/PJ", REPCOOPCONS.cliente.numcpfcnpj AS "CPF/CNPJ", 
REPCOOPCONS.CLIENTE.DESCNOMECLIENTE AS "Nome", REPCOOPCONS.CLIENTEINSTUNIDADE.idunidadeinst AS "Nº PA" FROM
REPCOOPCONS.CLIENTE 
JOIN REPCOOPCONS.CLIENTEINSTUNIDADE ON REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.IDCLIENTE = clienteinstunidade.idcliente
ORDER BY "Código Cliente"

CLIENTE COM RENDA
SELECT
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idclientesisbr AS "Código Cliente", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.BOLPESSOAFISICA AS "PF/PJ",
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.numcpfcnpj AS "CPF/CNPJ", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.DESCNOMECLIENTE AS "Nome", 
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.idunidadeinst AS "Nº PA", REPCOOPCONS.CLI_FONTERENDA.VALORRECEITABRUTAMENSAL AS "Renda mensal" 
FROM REPCOOPCONS.CLI_CLIENTEINSTUNIDADE, REPCOOPCONS.CLI_FONTERENDA

WHERE REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.IDCLIENTE =  REPCOOPCONS.CLI_FONTERENDA.IDCLIENTE
ORDER BY "Código Cliente"


CLIENTE COM CNAE
SELECT
CLIENTEINSTUNIDADE.IDCLIENTE,CLIENTEINSTUNIDADE.NUMCPFCNPJ,CLIENTEINSTUNIDADE.DESCNOMECLIENTE,
CLIENTEINSTUNIDADE.IDNIVELRISCO, CLIENTEINSTUNIDADE.CODCNAE,CLIENTEINSTUNIDADE.IDUNIDADEINST,
CLIENTEINSTUNIDADE.DATACADASTRAMENTOCLIENTE,CLIENTEINSTUNIDADE.DATAULTIMAATUALIZACAO 
FROM CLIENTEINSTUNIDADE
ORDER BY IDCLIENTE

CLIENTE PRODUTIVIDADE 
SELECT

REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.IDCLIENTE,
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.BOLPESSOAFISICA AS "PF/PJ",
REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.numcpfcnpj AS "CPF/CNPJ", REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.DESCNOMECLIENTE AS "Nome", 
REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.RENDAAGROPECUARIA12MESES,
REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.AREAHECTARE,
REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.QTDPRODUCAOHECTARE,
REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.IDEMPREENDIMENTO, REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.ANOREFERENCIA,
REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.IDUNIDADEINST 

FROM REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE,REPCOOPCONS.CLI_CLIENTEINSTUNIDADE

WHERE REPCOOPCONS.CLI_CLIENTEPRODUTIVIDADE.IDCLIENTE =  REPCOOPCONS.CLI_CLIENTEINSTUNIDADE.IDCLIENTE

ORDER BY IDCLIENTE

CLIENTE PRODUTOR
SELECT
CLIENTEPRODUTOR.IDCLIENTE,CLIENTEPRODUTOR.CODINSCRICAOPRODRURAL,
CLIENTEPRODUTOR.IDCATEGORIAPRODUTOR,CLIENTEPRODUTOR.IDUNIDADEINST 
FROM CLIENTEPRODUTOR
ORDER BY IDCLIENTE

CLIENTE FISICA
SELECT
CLIENTEFISICA.IDCLIENTE,CLIENTEFISICA.IDINSTITUICAO,CLIENTEFISICA.CODTIPOPROFISSAO,CLIENTEFISICA.CODESTADOCIVIL,
CLIENTEFISICA.CODREGIMECASAMENTO,CLIENTEFISICA.CODTIPOSEXO,CLIENTEFISICA.CODTIPOEXPOSICAO,CLIENTEFISICA.CODVINCULOEMPREGATICIO,
CLIENTEFISICA.CODNIVELEDUCACIONAL,CLIENTEFISICA.DATANASCIMENTO,CLIENTEFISICA.DESCNATURALIDADE,CLIENTEFISICA.DESCNACIONALIDADE
FROM CLIENTEFISICA
ORDER BY IDCLIENTE


